
#include <iostream>

using namespace std;

//Ejercicio 19
//Andres Enrique Lopez Rodriguez 00136419

int main() {
    
    int matriz[5][5];
    
    
    for (int i=0;i<5;i++){
        for (int j=0;j<5;j++){
            matriz[i][j]=0;
            
            
        }
    }            

    cout<<"Digite el numero de la casilla a00: ";
    cin>>matriz[0][0];
    
    cout<<"Digite el numero de la casilla a10: ";
    cin>>matriz[1][0];
    
    cout<<"Digite el numero de la casilla a11: ";
    cin>>matriz[1][1];
    
    cout<<"Digite el numero de la casilla a20: ";
    cin>>matriz[2][0];
    
    cout<<"Digite el numero de la casilla a21: ";
    cin>>matriz[2][1];
    
    cout<<"Digite el numero de la casilla a22: ";
    cin>>matriz[2][2];
    
    cout<<"Digite el numero de la casilla a30: ";
    cin>>matriz[3][0];
    
    cout<<"Digite el numero de la casilla a31: ";
    cin>>matriz[3][1];
    
    cout<<"Digite el numero de la casilla a32: ";
    cin>>matriz[3][2];
    
    cout<<"Digite el numero de la casilla a33: ";
    cin>>matriz[3][3];
    
    cout<<"Digite el numero de la casilla a40: ";
    cin>>matriz[4][0];
    
    cout<<"Digite el numero de la casilla a41: ";
    cin>>matriz[4][1];
    
    cout<<"Digite el numero de la casilla a42: ";
    cin>>matriz[4][2];
    
    cout<<"Digite el numero de la casilla a43: ";
    cin>>matriz[4][3];
    
    cout<<"Digite el numero de la casilla a44: ";
    cin>>matriz[4][4];
    
    cout<<matriz[0][0]<<" "<<matriz[0][1]<<" "<<matriz[0][2]<<" "<<matriz[0][3]<<" "<<matriz[0][4]<<" "<<endl;
    cout<<matriz[1][0]<<" "<<matriz[1][1]<<" "<<matriz[1][2]<<" "<<matriz[1][3]<<" "<<matriz[1][4]<<" "<<endl;
    cout<<matriz[2][0]<<" "<<matriz[2][1]<<" "<<matriz[2][2]<<" "<<matriz[2][3]<<" "<<matriz[2][4]<<" "<<endl;
    cout<<matriz[3][0]<<" "<<matriz[3][1]<<" "<<matriz[3][2]<<" "<<matriz[3][3]<<" "<<matriz[3][4]<<" "<<endl;
    cout<<matriz[4][0]<<" "<<matriz[4][1]<<" "<<matriz[4][2]<<" "<<matriz[4][3]<<" "<<matriz[4][4]<<" "<<endl;
    
    
    
    return 0;
}

