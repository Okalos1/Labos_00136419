#include <iostream>

using namespace std;

//EJERCICIO 11
//Andres Enrique Lopez Rodriguez 00136419

int numeromayor(int x){
    int aux=0;
    
    int arreglo[x];
    
    for (int i=0;i<x; i++){
     cout<< "Digite el numero: ";
     cin>>arreglo[i];
     
    };
    
    for (int i=0;i<x; i++){
        
        
        if(arreglo[i]>aux){
            aux=arreglo[i];
        }
    }
    cout<<"El numero mayor es: "<<aux<<endl;
    
    
}

int main(){
    
    int x=0;
    
    cout<<"Digite la cantidad de numero NATURALES a ingresar: ";
    cin>>x;
    
    numeromayor(x);
}
